            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="raisedBtn" layoutX="156.0" layoutY="123.0" onAction="#raisedBtnAction" prefHeight="54.0" prefWidth="88.0" style="-fx-background-color: #757575;" text="Raised" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="disabledBtn" disable="true" layoutX="280.0" layoutY="123.0" prefHeight="54.0" prefWidth="88.0" style="-fx-background-color: #e2e2e2;" text="Disabled">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
