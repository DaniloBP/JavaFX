<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #b5b5b5;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ButtonsController">
   <children>
      <JFXButton fx:id="flatBtn" layoutX="32.0" layoutY="123.0" prefHeight="54.0" prefWidth="88.0" style="-fx-background-color: #e2e2e2;" text="Flat">
